{
  "type": 1,
  "name": "React Native WebRtc Web Shim Test",
  "matrix": [
    [0, 0],
    [0, 1],
    [0, 2],
    [1, 0],
    [1, 1],
    [1, 2],
    [2, 0],
    [2, 1]
  ],
  "_simple_matrix": [[0, 0]],
  "permute": true,
  "grids": [
    {
      "type": "local",
      "url": "http://localhost:4444/wd/hub"
    }
  ],
  "tests": [
    {
      "name": "React Native WebRtc Web Shim Test Local Grid %ts",
      "description": "Cross-browser compatibility test for React Native WebRtc Web Shim",
      "tupleSize": 2,
      "testImpl": "com.ruddell.kite.RnWebRtcWebShimTest",
      "payload": {
        "url": "http://localhost:5000",
        "usersPerRoom": 2,
        "streamGuid": "Bgfu14m",
        "pageTitle": "React Native WebRtc Web Shim",
        "takeScreenshotForEachTest": true,
        "testTimeout": 20,
        "sfu": false,
        "meetingDuration": 5,
        "getStats": {
          "enabled": true,
          "statsCollectionTime": 10000,
          "statsCollectionInterval": 250,
          "peerConnections": ["[...window.component.state.peers.values()][0]"],
          "selectedStats": ["inbound-rtp", "outbound-rtp", "candidate-pair"]
        },
        "getCharts": {
          "enabled": true,
          "pathToGenerateChartsFile": "/generateCharts-min.js",
          "pathToChartBundleMin": "/chart.bundle.min.js",
          "chartsStatsInterval": 500,
          "peerConnections": ["[...window.component.state.peers.values()][0]"],
          "config": {
            "width": 500,
            "height": 500,
            "colors": [
              "#1DABE6",
              "#C3CED0",
              "#41ab48",
              "#E43034",
              "#000000",
              "#AF060F"
            ],
            "filter": ["inbound-rtp", "outbound-rtp", "candidate-pair"],
            "charts": [
              "receivedAudioBitrates",
              "receivedVideoBitrates",
              "sentAudioBitrates",
              "sentVideoBitrates",
              "audioPacketLoss",
              "videoPacketLoss",
              "audioJitter",
              "videoFramesSent",
              "audioLevel",
              "framesReceivedRate",
              "framesSentRate"
            ]
          }
        }
      }
    }
  ],
  "clients": [
    {
      "browserName": "chrome",
      "version": 116,
      "platform": "localhost",
      "_capabilities": {
        "acceptInsecureCerts": true
      }
    },
    {
      "browserName": "firefox",
      "version": 117,
      "platform": "localhost"
    },
    {
      "browserName": "safari",
      "platform": "localhost"
    }
  ]
}
